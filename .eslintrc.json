{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["solid"],
  "extends": [
    "standard-with-typescript",
    "eslint:recommended",
    "plugin:solid/typescript"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/no-non-null-assertion": "off"
    // "@typescript-eslint/strict-boolean-expressions": "off"
  }
}
